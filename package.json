{
  "name": "neo-breakout-cyberpunk",
  "version": "2.0.0",
  "description": "A futuristic cyberpunk-themed HTML5 Breakout game powered by Groq AI with advanced graphics, neon effects, and intelligent gameplay",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "build": "echo 'Build complete - static files ready'",
    "start": "vercel dev",
    "deploy": "vercel --prod",
    "lint": "eslint *.js",
    "test": "echo 'Tests coming soon'"
  },
  "keywords": [
    "breakout",
    "game",
    "html5",
    "canvas",
    "javascript",
    "cyberpunk",
    "neon",
    "ai",
    "groq",
    "futuristic",
    "neural",
    "gaming"
  ],
  "author": {
    "name": "George Pricop",
    "email": "contact@georgepricop.ro",
    "url": "https://github.com/Gzeu"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gzeu/html5-breakout-game.git"
  },
  "bugs": {
    "url": "https://github.com/Gzeu/html5-breakout-game/issues"
  },
  "homepage": "https://html5-breakout-game.vercel.app/",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "groq-sdk": "^0.4.0"
  },
  "devDependencies": {
    "@vercel/node": "^3.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "vercel": {
    "functions": {
      "api/groq-ai.js": {
        "runtime": "nodejs18.x",
        "maxDuration": 10
      }
    },
    "env": {
      "GROQ_API_KEY": "@groq-api-key"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Gzeu"
  }
}